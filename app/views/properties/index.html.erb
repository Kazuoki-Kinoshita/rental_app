<h1>物件一覧</h1>

<table>
  <ul>
    <tr>
      <th>物件名</th>
      <th>賃料</th>
      <th>住所</th>
      <th>築年数</th>
      <th>備考</th>
    </tr>
    <% @properties.each do |property| %>
    <tr>
      <td><%= property.name %></td>
      <td><%= property.rent %>円</td>
      <td><%= property.address %></td>
      <td><%= property.year %>年</td>
      <td><%= property.remark %></td>
    </tr>
  <% end %>
</table>
<%= link_to "物件を登録する", new_property_path %>